{
  "name": "bakareader",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "npm run test:lint && npm run test:coverage",
    "test:ci": "npm run test:lint && npm run test:coverage",
    "test:coverage": "jest --coverage",
    "test:flow": "flow",
    "test:lint": "eslint . --quiet",
    "test:unit": "jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "babel-preset-react-native-stage-0": "1.0.1",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.2",
    "react-native-fetch-blob": "0.10.8",
    "react-native-image-resizer": "1.0.0",
    "react-native-modal": "2.5.0",
    "react-native-vector-icons": "4.6.0",
    "react-navigation": "1.0.0-beta.11",
    "realm": "1.10.3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.1.0",
    "coveralls": "3.0.0",
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.19.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.2.1",
    "flow-bin": "0.52.0",
    "jest": "20.0.4",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(jpg|png|gif|eot|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/jest/FileStub.js"
    },
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "eslint-plugin-jsx-a11y"
    ]
  }
}
